@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    var ex = Model.Exception as System.Data.Entity.Infrastructure.DbUpdateException;
}

<h1 class="text-danger">傳入SQL Server的Update錯誤</h1>
<h2 class="text-danger">@ex.InnerException.InnerException.Message</h2>

<pre>@ex.ToString()</pre>